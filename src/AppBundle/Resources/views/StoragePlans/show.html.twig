{% extends '::baseadm.html.twig' %}

{% block body -%}
    <h5>StoragePlans</h5>

     <table  cellpadding="0" cellspacing="0" border="0" width="90%"  class="tablaAdm tableShow">
        <tbody>
            <tr>
                <th>Id</th>
                <td >{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tag</th>
                <td >{{ entity.tag }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td >{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Space</th>
                <td >{{ entity.space }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td >{{ entity.description | raw  }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('storageplans') }}" class="submit btnAdm lilaOscuro">
            Volver              
        </a>
    </li>
    <li>
        <a href="{{ path('storageplans_edit', { 'id': entity.id }) }}"  class="submit btnAdm rojoFuerte">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
