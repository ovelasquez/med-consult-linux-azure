     <table  cellpadding="0" cellspacing="0" border="0" width="90%"  class="tablaAdm tableShow">
        <tbody>
            <tr>
                <th>Id</th>
                <td >{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td >{{ entity.name }}</td>
            </tr>
            <!--tr>
                <th>Description</th>
                <td >{{ entity.description | raw}}</td>
            </tr-->
