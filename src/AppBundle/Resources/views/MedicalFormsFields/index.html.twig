{% extends '::baseadm.html.twig' %}

{% block body -%}
    <h5>MedicalFormsFields list</h5>

    <table cellpadding="0" cellspacing="0" border="0" width="90%" class="table-striped tablaAdm ">
        <thead>
            <tr>
                <th>Field</th>
                <th>Label</th>
                <th>Data</th>
                <th>Config</th>
                
                <th class=" col-lg-1 col-md-1 col-sm-1" colspan="2">OPERACIONES</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('medicalformsfields_show', { 'id': entity.id }) }}">{{ entity.field }}</a></td>
                <td>{{ entity.label }}</td>
                <td>{{ entity.data }}</td>
                <td>{{ entity.config }}</td>
                
                <td>
                        <a href="{{ path('medicalformsfields_show', { 'id': entity.id }) }}">ver</a>
                    </td>
                    <td>
                        <a href="{{ path('medicalformsfields_edit', { 'id': entity.id }) }}">editar</a>
                    </td>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('medicalformsfields_new') }}" class="submit submitAdm lilaOscuro">
                Crear una entrada
            </a>
        </li>
    </ul>
    {% endblock %}
