<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FormPartners" table="form_partners">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nameContact" type="string" column="name_contact" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="typeCompany" type="string" column="type_company" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phoneNumbers" type="string" column="phone_numbers" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hearAboutUs" type="string" column="hear_about_us" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="message" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateTime" type="datetime" column="date_time" nullable="false"/>
    <field name="ip" type="string" column="ip" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
