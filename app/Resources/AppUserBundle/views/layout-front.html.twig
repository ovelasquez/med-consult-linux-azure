<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}MEDECONSULT{% endblock %}</title>
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.png') }}" />
        <!-- Bootstrap -->
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/styles_exterior.css') }}" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/slick.css') }}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/slick-theme.css') }}"/>
        
        {% block stylesheets %}{% endblock %}
        
        <script type="text/javascript" src="{{ asset('js/vendor/jquery-1.11.3.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/slick.js') }}"></script>

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body>
               
        <div class="container-fluid">
            <div class="row" style="height:100%;">
                <div class="col-xs-2 barramenu">
                    <div class="logoPrincipal">
                        {% include "logo.html.twig" %}
                    </div><!--logoPricnipal-->
                    <ul>
                        <li class="espacioSuperior celeste "><a href="{{ url('comofunciona') }}">c√≥mo funciona <div class="hover"></div></a></li>
                        <li class="espacioSuperior dos rojo current"><a href="#">entrar<br>&HorizontalLine;&HorizontalLine;&HorizontalLine;&HorizontalLine;&HorizontalLine;<br>registro<div class="hover"></div></a></li>
                        <li class="espacioSuperior tres gris "><a href="{{ url('consultasservicios') }}">consultas<br>y servicios<div class="hover"></div></a></li>
                        <li class="espacioSuperior tres lila "><a href="{{ url('acercamedeconsult') }}">acerca<br>de medeconsult<div class="hover"></div></a></li>
                        <li class="espacioSuperior celeste "><a href="#">contacto<div class="hover"></div></a></li>
                    </ul>
                </div><!--barramenu-->
                <div class="col-xs-10 barrainfo">
                    <!-- inicio TOPE -->
                    {% include "barratope.html.twig" %}
                    <!-- fin TOPE -->
                    <!-- inicio INFO -->
                     <div class="row" style="height:80%; ">
                        <div class="col-xs-7 col-lg-8 modbg_izq" >
                            <div class="imagenRegistro"></div><!--imagenRegistro-->
                        </div>
                        <div class="col-xs-5 col-lg-4 modpq_der modpq_registro" >
                            <div>
                                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                    {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                                    <i class="fa fa-sign-out fa-1x textGray fa-fw margin-bottom"></i><a href="{{ path('fos_user_security_logout') }}" class="userTxt">
                                        {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                {% else %}
                                    <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
                                {% endif %}
                            </div>

                            {% if app.request.hasPreviousSession %}
                                {% for type, messages in app.session.flashbag.all() %}
                                    {% for message in messages %}
                                        <div class="flash-{{ type }}">
                                            {{ message }}
                                        </div>
                                    {% endfor %}
                                {% endfor %}
                            {% endif %}

                            <div>
                                {% block fos_user_content %}
                                {% endblock fos_user_content %}
                            </div>
                        </div><!--modpq_der modpq_registro-->
                    </div><!--row-->
                    <div class="row" style="height:10%; ">
                        <div class="col-xs-12 barrafooter barraf_registro">
                            <ul>
                                <li class="current"><a href="#">Registro</a><div class="arrow registro"></div></li>
                            </ul>
                        </div><!--barrafooter barraf_registro-->
                    </div><!--row-->
                    
                    
                    <!-- fin INFO -->
                </div>
            </div>
        </div>

        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>-->
        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('js/script.js') }}"></script>
        {% block javascripts %}{% endblock %}
        <script type="text/javascript">
            var current = 1;
            $(document).ready(function () {
                $('.your-class').slick({
                    dots: true,
                    arrows: true
                });
            });
        </script>
    </body>
</html>