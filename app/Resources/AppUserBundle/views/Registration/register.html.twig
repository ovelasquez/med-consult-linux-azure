{% set qP = app.request.query.all %}

{% extends qP.layout is not defined
    ? "FOSUserBundle::layout-login.html.twig" 
    : 'FOSUserBundle::layout-simple.html.twig' %}
    

{% block fos_user_content %}    
{% if qP.guess is defined %}
    {% include "FOSUserBundle:Registration:register_content_guess.html.twig"  with {'tkG': qP.tkG}  %}
{% else %}
    {% include "FOSUserBundle:Registration:register_content.html.twig" %}
{% endif %}    
{% endblock fos_user_content %}
