<script>
    $(function () {
        //alert("status")
        $(".menu-site-pub").click(function () {

            var display = ($('#nav-site-pub').is(':visible'));
            var visibility = ($('#nav-site-pub').css('visibility') != 'hidden');
            var status = (display && visibility);

            //alert(status + " " + $("#sub-menu-collapse").attr('aria-expanded'))



            $('#nav-site-pub #main-menu-navigation .nav-item a span').show();
            if ($("#sub-menu-collapse").attr('aria-expanded') === 'true') {
                $("#sub-menu-collapse").click();
            }

            if (status)
                $("#nav-site-pub").hide();
            else
                $("#nav-site-pub").show();
        });

        $("#sub-menu-collapse").click(function () {
            var display = ($('#nav-site-pub').is(':visible'));
            var visibility = ($('#nav-site-pub').css('visibility') != 'hidden');
            var status = (display && visibility);
            //alert(status +" -- "+ ($("#sub-menu-collapse").attr('aria-expanded') === 'true') +" -- "+  ($("#sub-menu-collapse").attr('aria-expanded')==undefined) )
            if (status && (!$("#sub-menu-collapse").attr('aria-expanded') === 'true' || $("#sub-menu-collapse").attr('aria-expanded') == undefined))
                $("#nav-site-pub").hide();


        });
    });

</script>


