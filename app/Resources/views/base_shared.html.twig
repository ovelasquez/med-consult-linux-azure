<script>
    $(function () {
        //alert("status")

        $(".menu-top").click(function () {

            var elem = $(this).prop("id");

            $(".menu-top").each(function () {
                if ($(this).prop("id") !== elem) {
                    if ($(this).parent().hasClass("dropdown")) {
                        $(this).parent().removeClass("open");
                        $(this).prop("aria-expanded", "false");
                    } else if ($("#nav-site-pub").is(':visible') || $("#nav-site-pub").css('visibility') != 'hidden') {
                        $("#nav-site-pub").hide();
                    }
                }

            });
            /**/
        });


        $("#menu-site-pub").click(function () {
            if ($(this).hasClass('is-active')) {
                $("#nav-site-pub").hide();
            } else {
                $("#nav-site-pub").show();
            }   /* /**/
        });

        $("#sub-menu-collapse").click(function () {
            if ($("#sub-menu-collapse").attr('aria-expanded') !== 'true') {
                $("#nav-site-pub").hide();
                $('body').removeClass('menu-open').addClass("menu-hide");
                $("#menu-site-pub-s").removeClass("is-active");
            }

        });
        $("#menu-site-pub-s").click(function () {

            if ($(this).hasClass('is-active')) {
                $("#nav-site-pub").hide();
                $('body').removeClass('menu-open').addClass("menu-hide");
                $(this).removeClass("is-active");
            } else {
                if ($("#sub-menu-collapse").attr('aria-expanded') === 'true')
                    $("#sub-menu-collapse").click();
                $("#nav-site-pub").show();
                $('body').addClass('menu-open').removeClass("menu-hide");
                $(this).addClass("is-active");
            }

        });


    });

</script>


